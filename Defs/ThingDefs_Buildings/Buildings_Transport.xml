  <!--======================================= Growzone Puller ===============================================-->
  <ThingDef ParentName="PRF_Puller_Base">
    <defName>PRF_GrowzonePuller_I</defName>
    <thingClass>ProjectRimFactory.AutoMachineTool.Building_HarvestPuller</thingClass>
    <label>growzone Puller</label>
    <statBases>
      <MarketValue>1200</MarketValue>
    </statBases>
    <passability>Impassable</passability>
    <description>Attach to end of grow zone, will pull set items within selected growzone. Will not pull from neighboring grow zones.</description>
